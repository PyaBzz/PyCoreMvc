@using System.Linq;
@using myCoreMvc.Models;

@model WorkPlan
@{ ViewBag.Title = "WorkPlan Details"; }

<a asp-controller="ListOfWorkPlans" asp-action="Index">Back</a>
@if (@Model != null)
{
    <h3>WorkPlan details</h3>
    <a asp-controller="EnterWorkPlan" asp-action="Index" asp-route-id="@Model.Id">Edit</a>
    <a asp-controller="DetailsOfWorkPlan" asp-action="Delete" asp-route-id="@Model.Id">Delete</a>
    <div>@Model.Name</div>
}
else
{
    <h5>WorkPlan not found!</h5>
}